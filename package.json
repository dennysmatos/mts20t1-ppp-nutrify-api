{
  "name": "mts20t1-ppp-nutrify",
  "version": "1.0.0",
  "description": "API REST para gerenciamento de alimentação, dieta e controle de calorias.",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --runInBand",
    "test:unit": "jest --runInBand test/unit --reporters=default --reporters=jest-junit",
    "test:functional": "mocha test/functional",
    "test:performance:dev": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_PERIOD=2s K6_WEB_DASHBOARD_EXPORT=test/performance/k6/reports/html-report.html k6 run test/performance/k6/registerFood.test.js -e BASE_URL=\"http://localhost:3000\" --vus 10 --duration 30s",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test:unit",
    "reports": "npm run coverage && npm run test:functional",
    "lint": "eslint .",
    "prettier": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1",
    "express-async-errors": "^3.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "supertest": "^6.3.3",
    "mocha": "^10.2.0",
    "chai": "^4.3.8",
    "nyc": "^15.1.0",
    "mochawesome": "^7.1.3",
    "jest-junit": "^13.0.0"
  }
}
